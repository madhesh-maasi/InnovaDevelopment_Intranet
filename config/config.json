{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "banner-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/banner/BannerWebPart.js",
          "manifest": "./src/webparts/banner/BannerWebPart.manifest.json"
        }
      ]
    },
    "feedback-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/feedback/FeedbackWebPart.js",
          "manifest": "./src/webparts/feedback/FeedbackWebPart.manifest.json"
        }
      ]
    },
    "innova-team-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/innovaTeam/InnovaTeamWebPart.js",
          "manifest": "./src/webparts/innovaTeam/InnovaTeamWebPart.manifest.json"
        }
      ]
    },
    "meeting-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/meeting/MeetingWebPart.js",
          "manifest": "./src/webparts/meeting/MeetingWebPart.manifest.json"
        }
      ]
    },
    "news-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/news/NewsWebPart.js",
          "manifest": "./src/webparts/news/NewsWebPart.manifest.json"
        }
      ]
    },
    "company-value-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/companyValue/CompanyValueWebPart.js",
          "manifest": "./src/webparts/companyValue/CompanyValueWebPart.manifest.json"
        }
      ]
    },
    "news-view-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/newsView/NewsViewWebPart.js",
          "manifest": "./src/webparts/newsView/NewsViewWebPart.manifest.json"
        }
      ]
    },
    "meeting-view-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/meetingView/MeetingViewWebPart.js",
          "manifest": "./src/webparts/meetingView/MeetingViewWebPart.manifest.json"
        }
      ]
    },
    "innova-team-view-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/innovaTeamView/InnovaTeamViewWebPart.js",
          "manifest": "./src/webparts/innovaTeamView/InnovaTeamViewWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BannerWebPartStrings": "lib/webparts/banner/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "FeedbackWebPartStrings": "lib/webparts/feedback/loc/{locale}.js",
    "InnovaTeamWebPartStrings": "lib/webparts/innovaTeam/loc/{locale}.js",
    "MeetingWebPartStrings": "lib/webparts/meeting/loc/{locale}.js",
    "NewsWebPartStrings": "lib/webparts/news/loc/{locale}.js",
    "CompanyValueWebPartStrings": "lib/webparts/companyValue/loc/{locale}.js",
    "NewsViewWebPartStrings": "lib/webparts/newsView/loc/{locale}.js",
    "MeetingViewWebPartStrings": "lib/webparts/meetingView/loc/{locale}.js",
    "InnovaTeamViewWebPartStrings": "lib/webparts/innovaTeamView/loc/{locale}.js"
  }
}
